<?xml version="1.0"?>
<ASSETS>

  <!-- This is a proper definition that the AssetManager will
       use to produce objects of this type.-->
  <ASSET 
    typeID ="Testing"
    dstRectW="80"
    dstRectH="80"
    textureID="Explosion"
    anim="true"
    animSpeed="100"
    collidable="true"
    numFrames="3"/>
  
  <!-- Asset lists are not defintions themselves, 
       they are for organizing pre-defined lists
       together to support randomization later.-->
  
  <ASSET_LIST ListID="TestingList">
    <ITEM AssetID="Testing" SpawnRate="50"/>
    
  </ASSET_LIST>
  
  
</ASSETS>